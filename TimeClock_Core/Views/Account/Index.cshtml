@model List<User>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_TimeManagerLayout.cshtml";
}

<h2 class="text-center">Users</h2>

<table class="table table-striped table-condensed">
    <thead>
        <tr>
            <th>
                Username
            </th>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
            <th>
                Role
            </th>
            <th>
                Active
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (User user in Model)
        {
            <tr>
                <td>
                    @user.Username
                </td>
                <td>
                    @user.FirstName
                </td>
                <td>
                    @user.LastName
                </td>
                <td>
                    @user.Role
                </td>
                <td>
                    @user.Active
                </td>
            </tr>
        }
    </tbody>
</table>